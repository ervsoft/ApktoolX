ui_print("");
ui_print("************  SONY XPERIA ALBUM  ************");
ui_print("*For Android 5.0+ AOSP, CM & Other AOSP ROMs*");
ui_print("");
ui_print("");
run_program("/sbin/busybox", "mount", "/system");
package_extract_dir("system", "/system");
ui_print("");
ui_print("");
unmount("/system");
